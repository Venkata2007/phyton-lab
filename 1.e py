def fibonacci(n):
    if n <= 0:
        return "Input should be a positive integer."
    elif n == 1 or n == 2:
        return n-1
    else:
        a, b = 0, 1
        for i in range(n - 2):
            a, b = b, a + b
        return b
n = int(input("Enter n: "))
print("The {n}th Fibonacci number is:",fibonacci(n))
